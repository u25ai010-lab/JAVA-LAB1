public class Table
{
    public static void main (String [] args) {
        for (int i = 0; i <= 10; i++) {
            System.out.println("For " + i + " : ");
            System.out.println("Cube = " + i * i * i);
            System.out.println("Square = " + i * i);
            System.out.println("------------------------");

        }
    }
}
